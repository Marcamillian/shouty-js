{
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
    }
  },
  "plugins": [
    "prettier",
  ],
  "rules": {
    "prettier/prettier": [
      "error", {"trailingComma": "es5", "singleQuote": true, "semi": false}
    ],
    "no-unused-vars": 1
  },
  "env": {
    "node": true,
    "es6": true,
  },
  "extends": [
    "eslint:recommended",
  ]
}
